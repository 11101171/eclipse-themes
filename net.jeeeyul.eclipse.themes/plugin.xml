<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.e4.ui.css.swt.theme">
      <stylesheet
            uri="css/empty.css">
         <themeid
               refid="net.jeeeyul.eclipse.themes.custom">
         </themeid>
         <osgiresourcelocator
               uri="platform:/plugin/net.jeeeyul.eclipse.themes/css/">
         </osgiresourcelocator>
      </stylesheet>
      <theme
            basestylesheeturi="css/jeeeyul-custom.css"
            id="net.jeeeyul.eclipse.themes.custom"
            label="Jeeeyul&apos;s themes - Custom Theme">
      </theme>
      <theme
            basestylesheeturi="css/minimalist.css"
            id="net.jeeeyul.eclipse.themes.minimalist"
            label="Jeeeyul&apos;s themes - Minimalist">
      </theme>
      <theme
            basestylesheeturi="css/minimalist-dark.css"
            id="net.jeeeyul.eclipse.themes.minimalist.dark"
            label="Jeeeyul&apos;s themes - Minimalist(Dark)">
      </theme>
      <theme
            basestylesheeturi="css/new-chrome.css"
            id="net.jeeeyul.eclipse.themes.chrome.new"
            label="Jeeeyul&apos;s themes - New Chrome">
      </theme>
   </extension>
   <extension
         id="id1"
         point="org.eclipse.e4.workbench.model">
      <processor
            beforefragment="true"
            class="net.jeeeyul.eclipse.themes.internal.JThemeProcessor">
      </processor>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.eclipse.ui.preferencePages.Views"
            class="net.jeeeyul.eclipse.themes.preference.JTPreperencePage"
            id="net.jeeeyul.eclipse.themes.preference.JTPreperencePage"
            name="Jeeeyul&apos;s Themes">
      </page>
      <page
            category="net.jeeeyul.eclipse.themes.preference.JTPreperencePage"
            class="net.jeeeyul.eclipse.themes.preference.JTPartStackPreferencePage"
            id="net.jeeeyul.eclipse.themes.preference.JTPartStackPreferencePage"
            name="Part Stack">
      </page>
   </extension>
   <extension
         point="org.eclipse.e4.ui.css.core.propertyHandler">
      <handler
            adapter="org.eclipse.e4.ui.css.swt.dom.CompositeElement"
            handler="net.jeeeyul.eclipse.themes.css.SashWidthHandler">
         <property-name
               name="jeeeyul-sash-width">
         </property-name>
      </handler>
      <handler
            adapter="org.eclipse.e4.ui.css.swt.dom.CTabFolderElement"
            handler="net.jeeeyul.eclipse.themes.css.JTabCSSPropertyHandler">
         <property-name
               name="jtab-border-color">
         </property-name>
         <property-name
               name="jtab-selected-border-color">
         </property-name>
         <property-name
               name="jtab-unselected-border-color">
         </property-name>
         <property-name
               name="jtab-border-radius">
         </property-name>
         <property-name
               name="jtab-spacing">
         </property-name>
         <property-name
               name="jtab-close-button-color">
         </property-name>
         <property-name
               name="jtab-close-button-hot-color">
         </property-name>
         <property-name
               name="jtab-close-button-active-color">
         </property-name>
         <property-name
               name="jtab-close-button-line-width">
         </property-name>
         <property-name
               name="jtab-unselected-tabs-background">
         </property-name>
         
         <property-name
               name="jtab-hover-tabs-background">
         </property-name>
         <property-name
               name="jtab-hover-border-color">
         </property-name>
         <property-name
               name="jtab-hover-color">
         </property-name>
         <property-name
               name="jtab-margin">
         </property-name>
         <property-name
               name="jtab-padding">
         </property-name>
         
          <property-name
               name="jtab-shadow-color">
         </property-name>
         
          <property-name
               name="jtab-shadow-radius">
         </property-name>
         
          <property-name
               name="jtab-shadow-position">
         </property-name>
         
         <property-name
               name="jtab-selected-text-shadow-color">
         </property-name>
         <property-name
               name="jtab-unselected-text-shadow-color">
         </property-name>
         <property-name
               name="jtab-hover-text-shadow-color">
         </property-name>
         
         <property-name
               name="jtab-selected-text-shadow-position">
         </property-name>
         <property-name
               name="jtab-unselected-text-shadow-position">
         </property-name>
         <property-name
               name="jtab-hover-text-shadow-position">
         </property-name>
         <property-name
               name="jtab-item-padding">
         </property-name>
         <property-name
               name="jtab-item-horizontal-spacing">
         </property-name>
      </handler>
   </extension>

</plugin>
